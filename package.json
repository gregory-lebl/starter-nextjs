{
  "name": "next-starter",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:cypress": "start-server-and-test dev 3000 cypress:open",
    "cypress:open": "cypress open",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "prepare": "husky install",
    "check-format": "prettier --check .",
